{
    "name": "emssyntax",
    "displayName": "EMS Syntax",
    "description": "Adds TIBCO EMS syntax language support to Visual Studio Code.",
    "version": "1.1.0",
    "engines": {
        "vscode": ">=0.9.0-pre.1"
    },
    "publisher": "PascalReitermann93",
    "icon": "images/icon.png",
    "repository": "https://github.com/pascalre/vscode-emssyntax",
    "contributes": {
        "languages": [{
            "id": "ems",
            "aliases": ["EMS"],
            "extensions": [".ems",".emsx"]
        }],
        "grammars": [{
            "language": "ems",
            "scopeName": "source.ems",
            "path": "./syntaxes/ems.tmLanguage.json"
        }]
    }
}
